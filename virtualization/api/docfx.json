{
  "build": {
    "content": [
      {
        "files": ["**/**.md", "**/**.yml"],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [], 
    "globalMetadata": {
      "breadcrumb_path": "/virtualization/api/breadcrumbs/toc.json",
      "extendBreadcrumb": true, 
      "feedback_system": "GitHub", 
      "feedback_github_repo": "MicrosoftDocs/Virtualization-Documentation", 
      "feedback_product_url": "https://github.com/MicrosoftDocs/Virtualization-Documentation/issues"
    },
    "fileMetadata": {
      "ms.topic": {
        "/api/**/*.md": "reference",
        "/hyper-v-on-windows/**/*.md": "article"
      },
      "ms.prod": {
        "/api/**/*.md": "windows",
        "/hyper-v-on-windows/**/*.md": "windows-10-hyperv",
        "/windowscontainers/**/index.yml": "windows",
        "/windowscontainers/**/faq.yml": "windows"
      }
    },
    "template": [],
    "dest": "virtualization/api"
  }
}
