{
  "build": {
    "content": [
      {
        "files": ["**/**.md", "**/**.yml"],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
        {
          "files": ["**/media/**"],
          "exclude": ["**/obj/**"]
        }
    ],
    "externalReference": [],
    "fileMetadata": {
      "ms.topic": {
        "/team-blog/**/*.md": "article"
      },
      "ms.prod": {
        "/team-blog/**/*.md": "virtualization"
      }
    },
    "template": "op.html",
    "dest": "virtualization/community",
    "globalMetadata": {
      "breadcrumb_path": "/virtualization/toc.json",
      "extendBreadcrumb": true, 
      "feedback_system": "GitHub", 
      "feedback_github_repo": "MicrosoftDocs/Virtualization-Documentation", 
      "feedback_product_url": "https://github.com/MicrosoftDocs/Virtualization-Documentation/issues"
    },  
    "extendBreadcrumb": true
  }
}